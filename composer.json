{
    "name": "eric-famiglietti/laravel-slack-events",
    "description": "Laravel package for integrating with the Slack Events API",
    "keywords": [
        "events api",
        "laravel",
        "slack"
    ],
    "homepage": "https://github.com/eric-famiglietti/laravel-slack-events",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Famiglietti",
            "email": "eric.famiglietti@gmail.com",
            "homepage": "http://ericfamiglietti.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/http": "^5.6",
        "illuminate/routing": "^5.6",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelSlackEvents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSlackEvents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSlackEvents\\SlackEventsServiceProvider"
            ]
        }
    }
}
